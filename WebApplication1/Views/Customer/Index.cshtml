@{
}

@model List<WebApplication1.Models.Customer>

    <h2>Bank Customers</h2>
    <table>
        <thead>
            <tr>
                <th>id</th>
                <th>name</th>
                <th>email</th>
                <th>phne</th>
                <th>country</th>
            </tr>
        </thead>
        <tbody>
        @foreach (var customer in Model)
        {
            <tr>
                <td>@customer.id</td>
                <td>@customer.name</td>
                <td>@customer.email</td>
                <td>@customer.phone</td>
                <td>@customer.country</td>
            </tr>
        }
        </tbody>
    </table>
